#!/bin/bash

AWS_PROFILE=default

excludes=" --exclude '.CFid' --exclude 'README.md' --exclude 'index.html.orig'  --exclude '.git/*' --exclude 'project.*' --exclude '.DS_Store' "

flags="--no-follow-symlinks --delete "

base="`basename $PWD`"
subdomain=${base}.dfwskyview.com


# Using --delete here since bucket is all synced now.
echo Syncing all content to $subdomain
eval aws s3 sync . s3://${subdomain} ${excludes} ${flags}

echo
echo skipped all of...
find . -type l
echo
echo DONT run ./invalidate UNTIL after review on netlify.
